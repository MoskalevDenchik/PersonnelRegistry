@model DM.PR.Common.Entities.Employee


<div class="well well-sm">
    <div class="row">
        <div class="col-md-2">
            <a href="~/Employees/Details/@Model.Id">
                <img src="~/Content/Images/UserImg.jpg" class="img-responsive img-circle" />
            </a>
        </div>
        <div class="col-md-10">
            <div class="text-right" style="margin-top:20px; margin-right:20px">
                <strong>@Model.FirstName</strong>
                <strong>@Model.MiddleName </strong>
                <strong>@Model.LastName</strong> 
            </div>
        </div>
    </div>
</div>


