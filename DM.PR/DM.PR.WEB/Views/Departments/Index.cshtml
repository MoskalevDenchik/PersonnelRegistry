        @Html.ActionLink("Create", "Create", null, new { @class = "btn btn-default" })
    </div>
</div>


<table class="table table-condensed table-bordered  table-hover" style="background-color:white">
    <thead>
        <tr class="text-center" @*style="font-size:small"*@>
            <td>ID</td>
            <td>ParentId</td>
            <td>Name</td>
            <td>Adresss</td>
            <td>Numbers</td>
            <td>Description</td>
            <td>Edit</td>
            <td>Delete</td>
        </tr>
    </thead>
    <tbody class="text-center text-info" style="font-size:small; margin-bottom:2px">
        @Html.Action("List")
    </tbody>
