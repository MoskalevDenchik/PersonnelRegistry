@model Employee

@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>


<div class="row">

    <div class="col-md-2">
        @Html.Label("FirstName")
        <input type="text" name="FirstName" value="" />
    </div>
    <div class="col-md-2">
        @Html.Label("LastName")
        <input type="text" name="LastName" value="" />
    </div>
    <div class="col-md-2">
        @Html.Label("Middlename")
        <input type="text" name="Middlename" value="" />
    </div>
    <div class="col-md-2">
        @Html.Label("WorkTime")
        <input type="text" name="WorkTime" value="" />
    </div>
    <div class="col-md-2">
        @Html.Label("IsWorking")
        <input type="checkbox" name="WorkTime" value="" />
    </div>


</div>



<div id="EmployeeList">


</div>


<input type="button" id="Search" value="Search" />

<script src="~/Scripts/Search.js"></script>