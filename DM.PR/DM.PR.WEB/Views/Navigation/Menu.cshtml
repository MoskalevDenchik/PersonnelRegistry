@model IEnumerable<DM.PR.WEB.Models.Navigation.NavigationMenuViewModel>


<div>
    <ul id="menu">
        <li>
            <a href="#" data-department="0" name="0">Все</a>
        </li>
        @foreach (var item in Model)
        {
            <li>
                <a href="#" data-department="@item.Id">@item.Name</a>
            </li>
        }
    </ul>
</div>