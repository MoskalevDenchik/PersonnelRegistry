@model IEnumerable<string>
<div class="btn-group btn-group-lg btn-group-vertical ">
    @Ajax.RouteLink("ALL", new { controller = "Employees", action = "List" },
        new AjaxOptions { UpdateTargetId = "ListOfEmployees" }, new { @class = "btn btn-default" })
    @foreach (var link in Model)
    {
        @Ajax.RouteLink(link, new { controller = "Employees", action = "List", department = link },
                      new AjaxOptions { UpdateTargetId = "ListOfEmployees" }, new { @class = "btn btn-default" })
    }
</div>
